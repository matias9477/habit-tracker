{
  "expo": {
    "name": "Consistency",
    "slug": "consistency",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#4CAF50"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matiasturra.consistency",
      "buildNumber": "5",
      "infoPlist": {
        "NSUserNotificationUsageDescription": "This app uses notifications to remind you about your daily habits.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.matiasturra.consistency",
      "versionCode": 2,
      "permissions": [
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#4CAF50"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5560a7a3-b43d-4ab9-a47b-c9da50e49725"
      }
    },
    "owner": "matias.turra"
  }
}
